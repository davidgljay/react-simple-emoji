{
  "name": "react-simple-emoji",
  "version": "0.0.3",
  "description": "Simple emoji picker.",
  "main": "dist/react-emoji-picker.js",
  "scripts": {
    "clean": "rimraf lib dist .nyc_output || rm -rf lib dist .nyc_output",
    "lint": "eslint src",
    "test": "ava -v",
    "test:watch": "ava -v -- --watch",
    "test:cov": "nyc npm test",
    "build:lib": "babel src --out-dir lib",
    "build:umd:min": "webpack src/index.js dist/react-emoji-picker.js --config webpack.production.js",
    "build": "npm run build:lib && npm run build:umd:min",
    "prepublish": "npm run clean && npm run build"
  },
  "ava": {
    "require": "babel-core/register",
    "files:": "test/*.spec.js",
    "babel": "inherit"
  },
  "author": "Peng Jie <dean.leehom@gmail.com> (https://github.com/neighborhood999)",
	"repository": {
    "type": "git",
    "url": "https://github.com/neighborhood999/react-simple-emoji.git"
  },
  "keywords": [
    "react",
    "emoji"
  ],
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.10.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "get-emoji": "^2.0.0",
    "json-loader": "^0.5.4",
    "nyc": "^6.4.4",
    "react-addons-test-utils": "^15.1.0",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "babel-cli": "^6.8.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "peerDependencies": {
    "react": "^0.14.8 || ^15.1.0",
    "react-dom": "^0.14.8 || ^15.1.0"
  },
  "files": [
    "lib/",
    "dist/"
  ],
  "license": "MIT"
}
